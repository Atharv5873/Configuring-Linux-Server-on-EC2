$TTL    86400
@       IN      SOA     ns1.example.ddns.net. root.localhost. (
                           177          ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                          86400 )       ; Negative Cache TTL
;
@       IN      NS      ns1.example.ddns.net.
ns1     IN      A       X.X.X.X
mail    IN      MX 10   mail.example.ddns.net.
example.ddns.net.  IN      A       X.X.X.X
www     IN      A       X.X.X.X
mail    IN      A       X.X.X.X
